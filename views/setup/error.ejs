<% if(!locals.page){
    page = "";
} %>
<!doctype html>
<html>
    <head>
        <link rel="stylesheet" type="text/css" href="../CSS/setup.css">    
    </head>
    <body>
    <div id="wrapper">
        <div id="container">
            <h4>Error</h4>
            <h1>
            <% if(locals.error){ %>
                <%= error.reason %>
            <% } %>
            </h1> 
            <div class="btn prev-btn">
                <a href="./<%= page %>" class="btn-wrapper">
                    <svg id="arrow_left" viewBox="5 5 14 14">
                        <title>keyboard_arrow_right</title>
                        <path d="M8.578 16.359l4.594-4.594-4.594-4.594 1.406-1.406 6 6-6 6z"></path>
                    </svg> 
                    <span>Previous</span>  
                </a>
            </div>          
        </div>
    </div>
    </body>
</html>
